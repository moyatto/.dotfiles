# config: move
setopt auto_cd

# config: match
zstyle ':completion:*' matcher-list 'm:{a-z}={A-Z}'

# alias: misc
alias sz='source ~/.zshrc'
alias ls='ls -a --color'
alias ll='ls -al'

# alias: git
alias ga='git add'
alias gl='git log'
alias gd='git diff'
alias gs='git status'
alias gc='git commit -m'
alias gsh='git push'
alias gll='git pull'

# alias: vim
alias v='vim'
alias vi='vim'
alias vv='vim ~/.vimrc'
alias vz='vim ~/.zshrc'

